---
interface Props {
    data: JsonObject;
}

type JsonValue =
    | string
    | number
    | boolean
    | null
    | { [key: string]: JsonValue }
    | JsonValue[];

type JsonObject = { [key: string]: JsonValue };

const { data } = Astro.props as Props;
---

<script type="application/ld+json" is:inline set:html={JSON.stringify(data)} />
